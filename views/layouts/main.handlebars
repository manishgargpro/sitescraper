	$(document).on("click", ".delete", function () {
		var id = $(this).parent().attr("id");
		var articleId = $(this).parent().parent().parent().attr("id");
		console.log(id);
		console.log(articleId);
		$.ajax({
	    url: "/delete/" + id,
	    type: "DELETE",
	    data: {
